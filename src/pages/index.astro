---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Best Studio">
	<main>
		<header
			class="py-3 px-10 flex items-center fixed top-0 w-full justify-between z-40 text-white"
		>
			<div class="flex flex-grow basis-0">
				<svg
					class="h-6 w-32"
					xmlns="http://www.w3.org/2000/svg"
					xmlns:xlink="http://www.w3.org/1999/xlink"
					viewBox="0 0 150 150"
					><g id="Capa_2" data-name="Capa 2"
						><g id="Capa_1-2" data-name="Capa 1"
							><image
								width="150"
								height="150"
								xlink:href="data:image/jpeg;base64,"
							/></g
						></g
					></svg
				>
				<!-- <img src="/besstudiologo.jpg" alt="Logo"> -->
			</div>

			<nav>
				<ul
					class="flex text-sm [&>li>a]:text-white [&>li>a]:font-medium [&>li>a]:inline-block [&>li>a]:px-4 [&>li>a]:py-2 [&>li>a]:whitespace-nowrap"
				>
					<li><a href="#">Crossfit</a></li>
					<li><a href="#">Gym</a></li>
					<li><a href="#">Powerlifting</a></li>
					<li><a href="#">Sweat</a></li>
					<li><a href="#">Pilates</a></li>
				</ul>
			</nav>

			<nav class="flex flex-grow justify-end basis-0">
				<ul
					class="flex text-sm [&>li>a]:text-white [&>li>a]:font-medium [&>li>a]:inline-block [&>li>a]:px-4 [&>li>a]:py-2 [&>li>a]:whitespace-nowrap"
				>
					<li><a href="#">Best Studio</a></li>
					<li><a href="#">Tarifas</a></li>
					<li><a href="#">Horarios</a></li>
					<li><a href="#">Contacto</a></li>
				</ul>
			</nav>
		</header>

		<section
			class="bg-black h-screen w-screen text-center overflow-hidden relative"
		>
			<div class="z-30 relative h-full flex flex-col">
				<header>
					<h2 class="text-white pt-40 text-[40px] font-medium">
						King of Gyms
					</h2>
					<p class="text-white text-sm">
						Reserva un clase de prueba hoy mismo
					</p>
				</header>

				<footer
					class="flex flex-col flex-grow flex-nowrap justify-end pb-20"
				>
					<div>
						<a
							class="border-[3px] border-white bg-white/5 backdrop-blur-sm text-sm rounded font-medium
							text-white px-12 py-2 inline-block hover:bg-white hover:text-black transition-colors"
							href="#">Reserva una clase</a
						>
					</div>
				</footer>
			</div>

			<div class="absolute top-0 bottom-0 h-full w-full z-10">
				<video
					autoplay
					muted
					loop
					src="/crossfit-2.mp4"
					class="object-cover w-full h-full"></video>
			</div>
		</section>
	</main>
</Layout>

<style></style>
