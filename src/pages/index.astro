---
import Header from "../components/Header.astro";
import Layout from "../layouts/Layout.astro";
import Landing from "../components/Landing.astro";
import Coach from "../components/Coach.astro";
import Expectations from "../components/Expectations.astro";
---

<Layout title="Best Studio">
	<Header class="h-12 w-full"/>

	<main class="snap-y snap-mandatory relative w-full h-screen overflow-y-auto overflow-x-hidden scroll-smooth">
		<div class="snap-center">
			<Landing />
		</div>
		<div class="snap-start">
			<Coach />
		</div>
 		<div class="snap-center">
			<Expectations />
		</div>

	</main>
</Layout>

